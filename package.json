{
  "name": "sitemanager-reactjs",
  "version": "0.2.0",
  "description": "SiteManager built in ReactJS",
  "main": "build/server/main.js",
  "engines": {
    "node": "~6.9.1"
  },
  "scripts": {
    "clean": "rimraf build",
    "development": "babel-node --presets='latest' --plugins='transform-flow-strip-types,transform-object-rest-spread' ./tools/development",
    "build": "npm run clean && npm run build:client && npm run build:universal && npm run build:server",
    "build:client": "babel-node --presets='latest' --plugins='transform-flow-strip-types' `npm bin`/webpack --config ./tools/webpack/client.config.js",
    "build:universal": "babel-node --presets='latest' --plugins='transform-flow-strip-types' `npm bin`/webpack --config ./tools/webpack/universalMiddleware.config.js",
    "build:server": "babel-node --presets='latest' --plugins='transform-flow-strip-types' `npm bin`/webpack --config ./tools/webpack/server.config.js",
    "start": "node -r 'babel-polyfill' build/server",
    "deploy": "node tools/scripts/deploy",
    "lint": "eslint src",
    "flow": "flow",
    "flow:report": "flow-coverage-report -i 'src/**/*.js' -t html -t json -t text",
    "flow:remove": "node ./tools/flow/removeTypes.js && eslint --fix src"
  },
  "author": "ConstantCo",
  "license": "UNLICENSED",
  "devDependencies": {
    "assets-webpack-plugin": "3.5.1",
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.3.2",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-class-properties": "6.23.0",
    "babel-plugin-transform-es2015-destructuring": "6.23.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-latest": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "chalk": "^1.1.3",
    "chokidar": "1.6.1",
    "cross-env": "^3.1.3",
    "css-loader": "0.26.1",
    "dotenv": "4.0.0",
    "eslint": "3.16.1",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-flowtype": "2.30.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.10.0",
    "flow-bin": "0.39.0",
    "flow-coverage-report": "0.3.0",
    "flow-remove-types": "1.2.0",
    "glob": "^7.1.1",
    "happypack": "3.0.3",
    "json-loader": "0.5.4",
    "koa-webpack-middleware": "^1.0.1",
    "md5": "2.2.1",
    "moment": "^2.15.2",
    "node-notifier": "5.0.2",
    "pad": "^1.0.2",
    "react-hot-loader": "3.0.0-beta.6",
    "regenerator-runtime": "0.10.3",
    "rimraf": "2.6.0",
    "style-loader": "0.13.1",
    "sw-precache-webpack-plugin": "^0.9.0",
    "url-loader": "0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "1.10.1",
    "webpack-hot-middleware": "2.17.0",
    "webpack-md5-hash": "0.0.5",
    "webpack-node-externals": "1.5.4"
  },
  "dependencies": {
    "app-root-path": "2.0.1",
    "bluebird": "^3.4.6",
    "code-split-component": "1.0.1",
    "compression": "1.6.2",
    "graphql": "^0.9.1",
    "graphql-relay": "^0.5.1",
    "graphql-sequelize": "^5.2.1",
    "isomorphic-relay": "^0.7.3",
    "joi": "^10.2.2",
    "koa": "next",
    "koa-compress": "next",
    "koa-convert": "^1.2.0",
    "koa-favicon": "2",
    "koa-graphql": "latest",
    "koa-helmet": "^3.1.0",
    "koa-mount": "next",
    "koa-static": "next",
    "normalize.css": "5.0.0",
    "path": "0.12.7",
    "pg": "^6.1.0",
    "pg-hstore": "^2.3.2",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-helmet": "4.0.0",
    "react-relay": "^0.10.0",
    "react-router": "4.0.0-alpha.5",
    "sequelize": "^3.24.6",
    "serialize-javascript": "1.3.0",
    "source-map-support": "0.4.11"
  },
  "metadata": {
    "graphql": {
      "schema": "./dist/server/schema.json"
    }
  }
}
