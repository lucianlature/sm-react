{
  "name": "sitemanager-reactjs",
  "version": "0.1.0",
  "description": "SiteManager built in ReactJS",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=development node -r 'babel-register' ./server",
    "start-dev": "npm run start-dev-hmr",
    "start-dev-single-page": "node_modules/.bin/http-server src/static",
    "start-dev-hmr": "node_modules/.bin/webpack-dev-server --progress --inline --hot --history-api-fallback --port 3000",
