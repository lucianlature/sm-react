{
  "name": "sitemanager-reactjs",
  "version": "0.2.0",
  "description": "SiteManager built in ReactJS",
  "main": "build/server/main.js",
  "engines": {
    "node": "~6.9.1"
  },
  "scripts": {
    "removetypes": "node ./tools/flow/removeTypes.js && eslint --fix src",
    "typecheck": "flow",
    "lint": "eslint src",
    "clean": "rimraf build",
    "development": "babel-node --presets='latest' --plugins='transform-flow-strip-types' ./tools/development",
    "build:client": "babel-node --presets='latest' --plugins='transform-flow-strip-types' `npm bin`/webpack --config ./tools/webpack/client.config.js",
    "build:universal": "babel-node --presets='latest' --plugins='transform-flow-strip-types' `npm bin`/webpack --config ./tools/webpack/universalMiddleware.config.js",
    "build:server": "babel-node --presets='latest' --plugins='transform-flow-strip-types' `npm bin`/webpack --config ./tools/webpack/server.config.js",
    "build": "npm run clean && npm run build:client && npm run build:universal && npm run build:server",
    "start": "node -r 'babel-polyfill' build/server"
  },
  "author": "ConstantCo",
  "license": "UNLICENSED",
  "devDependencies": {
    "assets-webpack-plugin": "3.5.0",
    "babel-cli": "6.18.0",
    "babel-core": "6.18.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-class-properties": "6.18.0",
    "babel-plugin-transform-es2015-destructuring": "6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "chalk": "^1.1.3",
    "chokidar": "1.6.1",
    "colors": "1.1.2",
    "cross-env": "^3.1.3",
    "css-loader": "0.25.0",
    "dotenv": "2.0.0",
    "eslint": "3.8.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-flowtype": "2.25.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "flow-bin": "0.33.0",
    "flow-coverage-report": "0.1.0",
    "flow-remove-types": "1.0.4",
    "happypack": "2.2.1",
    "json-loader": "0.5.4",
    "koa-webpack-middleware": "^1.0.1",
    "md5": "2.2.1",
    "moment": "^2.15.2",
    "node-notifier": "4.6.1",
    "pad": "^1.0.2",
    "promisify-node": "0.4.0",
    "react-hot-loader": "3.0.0-beta.6",
    "recursive-readdir": "2.1.0",
    "regenerator-runtime": "0.9.5",
    "rimraf": "2.5.4",
    "spawn-monitor": "^1.1.5",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.0",
    "webpack-md5-hash": "0.0.5",
    "webpack-node-externals": "1.5.4"
  },
  "dependencies": {
    "app-root-path": "2.0.1",
    "babel-eslint": "7.1.0",
    "babel-loader": "6.2.7",
    "code-split-component": "1.0.0",
    "compression": "1.6.2",
    "eslint-plugin-import": "2.0.1",
    "koa": "next",
    "koa-compress": "next",
    "koa-convert": "^1.2.0",
    "koa-favicon": "2",
    "koa-helmet": "^2.0.0",
    "koa-mount": "next",
    "koa-static": "next",
    "normalize.css": "5.0.0",
    "path": "0.12.7",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-helmet": "3.1.0",
    "react-router": "4.0.0-alpha.5",
    "serialize-javascript": "1.3.0",
    "source-map-support": "0.4.5"
  },
  "metadata": {
    "graphql": {
      "schema": "./dist/server/schema.json"
    }
  }
}
