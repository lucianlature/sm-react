{
  "presets": [
    "react",
    "es2015",
    "stage-0"
  ],
  "plugins": [
    "babel-relay-plugin-loader",
    "react-hot-loader/babel",
    "transform-class-properties",
    "transform-object-rest-spread",
    "transform-async-to-generator",
    "transform-decorators-legacy",
    [ "transform-es2015-classes", { "loose": true } ],
    "transform-runtime"
  ],
  "ignore": [
    "node_modules"
  ]
}
